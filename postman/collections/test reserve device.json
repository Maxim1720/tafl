		{
			"name": "create zone",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "dc52f5d1-5b2c-4777-83c3-8d50c69b2222",
						"exec": [
							"const zone = {",
							"    name: pm.variables.replaceIn(\"{{$randomCompanyName}}\")",
							"}",
							"pm.environment.set(\"zone\", zone);",
							"",
							"pm.request.body.raw = zone;"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "52e1f0ac-788a-4af1-9bd1-54ee8184d920",
						"exec": [
							"pm.test(\"Zone was created\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"",
							"var requestDetails = {",
							"  url: pm.response.json()._links.zone.href,",
							"  method: 'GET',",
							"  headers: {",
							"    'Authorization': 'Bearer ' + pm.environment.get(\"per-token\")",
							"  }",
							"};",
							"",
							"",
							"var zone = {}",
							"",
							"pm.sendRequest(requestDetails, function(err, response){",
							"    if(err){",
							"        console.log(err);",
							"    }",
							"    else{",
							"        zone = response.json();",
							"    }",
							"});",
							"",
							"pm.environment.set(\"zone\", zone);"
						],
						"type": "text/javascript"
					}
				}
			],
			"id": "0cbb061a-dbc1-460c-8874-56f0a299c98a",
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"auth": {
					"type": "bearer",
					"bearer": [
