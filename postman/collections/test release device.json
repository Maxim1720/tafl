				{
					"listen": "test",
					"script": {
						"id": "945cc405-7b56-41e4-879d-4c8b544f049a",
						"exec": [
							"pm.test(\"Status code is 201\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"var requestDetails = {",
							"  url: pm.response.json()._links.self.href,",
							"  method: 'GET',",
							"  header: {",
							"    'Authorization': 'Bearer ' + pm.environment.get(\"per-token\")",
							"  }",
							"};",
							"",
							"pm.sendRequest(requestDetails, function(err, response){",
							"    if(err){",
							"        console.log(err);",
							"    }",
							"    else{",
							"        ",
							"    }",
							"})",
							"",
							"",
							"var href = pm.response.json()._links.self.href;",
							"var hrefSplit = href.split('/');",
							"var id = hrefSplit[hrefSplit.length - 1];",
							"pm.environment.set(\"device_id\", id);",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "f263b426-b469-4f52-87b4-94e3cf460307",
						"exec": [
							"const device = {",
							"    isOccupied:true,",
							"    zoneId: pm.environment.get(\"zone\").id",
							"}",
							"",
							"pm.request.body.raw = device;",
							"",
							""
						],
						"type": "text/javascript"
					}
				}
			],
